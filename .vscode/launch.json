    {
      "name": "The_basics_of_OOP: Class_and_object", // — Название конфигурации (отображается в выпадающем списке отладки).
      "type": "lldb", // — Указывает тип отладчика (LLDB для macOS/Linux).
      "request": "launch", // — Запуск программы с нуля (альтернатива: "attach" для подключения к работающему процессу).
      "args": [], // — Аргументы командной строки для программы (пустой список = без аргументов).
      "program": "${workspaceFolder}/build/Debug/bin/ExampleClass", // — Абсолютный путь к исполняемому файлу:
      "cwd": "${workspaceFolder}", // — Рабочая директория при запуске (корень проекта).
